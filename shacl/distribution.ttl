@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix cogs: <http://www.ons.gov.uk/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix spdx: <http://spdx.org/rdf/terms#> .
@prefix wdrs: <http://www.w3.org/2007/05/powder-s#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<#DatasetShape> a sh:NodeShape ;
    sh:targetClass dcat:Distribution ;
    sh:property [
        sh:path dcterms:title ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one dcterms:title." ;
        sh:severity sh:Warning ;
    ] ;
       sh:property [
        sh:path dcterms:description ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one dcterms:description." ;
        sh:severity sh:Warning ;
    ] ;
        sh:property [
        sh:path dcterms:license ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one dcterms:issued." ;
        sh:severity sh:Warning ;
    ] ;
       sh:property [
        sh:path dcterms:creator ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one dcterms:creator." ;
        sh:severity sh:Info ;
    ] ;
       sh:property [
        sh:path dcterms:issued ;
        sh:datatype xsd:dateTime ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one dcterms:issued." ;
        sh:severity sh:Info ;
    ] ;
    sh:property [
        sh:path dcterms:modified ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one dcterms:modified." ;
        sh:severity sh:Info ;
    ] ;
        sh:property [
        sh:path dcat:isDistribtionOf ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one dcat:isDistribtionOf." ;
        sh:severity sh:Info ;
    ] ;
        sh:property [
        sh:path dcat:mediaType ;
        sh:path dcterms:MediaType ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one dcat:mediaType." ;
        sh:severity sh:Info ;
    ] ;
        sh:property [
        sh:path dcterms:downloadURL ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one dcterms:downloadURL." ;
        sh:severity sh:Info ;
    ] ;
        sh:property [
        sh:path dcat:byteSize ;
        sh:datatype xsd:nonNegativeInteger ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one dcat:byteSize." ;
        sh:severity sh:Info ;
    ] ;
        sh:property [
        sh:path spdx:checksum ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one spdx:checksum." ;
        sh:severity sh:Info ;
     ];
        sh:property [
        sh:path wdrs:describedBy ;
        sh:path rdfs:Resource ;
        sh:minCount 1 ;
        sh:message "A dcat:Distribution must have at least one wdrs:describedBy." ;
        sh:severity sh:Info ;
    ] .