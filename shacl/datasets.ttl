@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix cogs: <http://www.ons.gov.uk/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix adms: <http://www.w3.org/ns/adms#> .

<#DatasetShape> a sh:NodeShape ;
    sh:targetClass cogs:Dataset ;
    sh:property [
        sh:path dcterms:identifier ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:identifier." ;
        sh:severity sh:Warning ;
    ];
    sh:property [
        sh:path dcterms:title ;
        sh:datatype: xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:title." ;
        sh:severity sh:Warning ;
    ];
     sh:property [
        sh:path dcterms:description ;
        sh:datatype: xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:description." ;
        sh:severity sh:Warning ;
    ];
    sh:property [
        sh:path dcterms:publisher ;
        sh:datatype: xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:publisher." ;
        sh:severity sh:Warning ;
    ];
    sh:property [
        sh:path dcterms:license ;
        #sh:path dcterms:LicenseDocument ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:license." ;
        sh:severity sh:Warning ;
    ];
     sh:property [
        sh:path dcat:distribution;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcat:distribution." ;
        sh:severity sh:Info ;
    ];
    sh:property [
        sh:path dcterms:creator ;
        #sh:foaf:Agent ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:creator." ;
        sh:severity sh:Info ;
    ];
     sh:property [
        sh:path dcat:contactPoint ;
        sh:path dcat:Dataset ;
        #sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcat:contactPoint." ;
        sh:severity sh:Info ;
     ];
      sh:property [
        sh:path dcterms:issued ;
        sh:datatype: xsd:dateTime ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:issued." ;
        sh:severity sh:Info ;
    ];
       sh:property [
        sh:path dcterms:modified ;
        sh:datatype: xsd:dateTime ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:modified." ;
        sh:severity sh:Info ;
    ];
     sh:property [
        sh:path dcat:keyword ;
        rdfs:Literal: xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcat:keyword." ;
        sh:severity sh:Info ;
    ];
     sh:property [
        sh:path dcat:theme ;
        #skos:Concept ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcat:theme." ;
        sh:severity sh:Info ;
    ];
      sh:property [
        sh:path dcterms:accrualPeriodicity ;
        sh:path dcterms:Frequency ;
        #sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:accrualPeriodicity." ;
        sh:severity sh:Info ;
    ];
     sh:property [
        sh:path dcterms:spatial ;
        sh:path dcterms:Location ;
        #sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:spatial." ;
        sh:severity sh:Info ;
    ];
    sh:property [
        sh:path dcterms:temporal ;
        sh:path dcterms:PeriodOftime ;
        #sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:temporal." ;
        sh:severity sh:Info ;
    ];
     sh:property [
        sh:path dcat:inSeries ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcat:inSeries." ;
        sh:severity sh:Info ;
    ];
    sh:property [
        sh:path dcat:hasCurrentVersion ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset should have a dcat:hasCurrentVersion." ;
        sh:severity sh:Info ;
    ];
     sh:property [
        sh:path dcat:hasVersion ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset should have a dcat:hasVersion." ; 
        sh:severity sh:Info ;  
    ];
     sh:property [
        sh:path dcat:version ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset should have a dcat:version." ;
        sh:severity sh:Info ;
    ];
     sh:property [
        sh:path adms:versionNotes ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset should have a adms:versionNotes ." ; 
        sh:severity sh:Info ;
    ];
      sh:property [
        sh:path dcat:prev ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset should have a dcat:prev." ;
        sh:severity sh:Info ;
    ];
      sh:property [
        sh:path adms:status ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset should have a adms:status." ;
        sh:severity sh:Info ;
    ];
        sh:property [
        sh:path dcat:landingPage ;
        #sh:foaf:Document ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcat:landingPage." ;
        sh:severity sh:Info ;
     ];
        sh:property [
        sh:path dcterms:isReferencedBy ;
        sh:datatype: xsd:string ;
        sh:minCount 1 ;
        sh:message: "A cogs:Dataset must have a dcterms:isReferencedBy." ;
        sh:severity sh:Info ;
    ].