{
  "@context": {
    "@language": "en",
    "dcterms": "http://purl.org/dc/terms/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "onsns": "https://example.org/ons/ns#",
    "vcard": "http://www.w3.org/2006/vcard/ns#",
    "time": "http://www.w3.org/2006/time#",
    "identifier": {
      "@id": "dcterms:identifier",
      "@language": null
    },
    "title": {
      "@id": "dcterms:title"
    },
    "summary": {
      "@id": "dcterms:abstract"
    },
    "description": {
      "@id": "dcterms:description",
      "@type": "https://www.iana.org/assignments/media-types/text/markdown"
    },
    "landing_page": {
      "@id": "dcat:landingPage",
      "@type": "@id"
    },
    "created": {
      "@type": "xsd:dateTime",
      "@id": "dcterms:created"
    },
    "issued": {
      "@type": "xsd:dateTime",
      "@id": "dcterms:issued"
    },
    "modified": {
      "@type": "xsd:dateTime",
      "@id": "dcterms:modified"
    },
    "next_release": {
      "@type": "xsd:dateTime",
      "@id": "ons:nextRelease"
    },
    "publisher": {
      "@type": "@id",
      "@id": "dcterms:publisher",
      "@context": {
        "@base": "https://www.gov.uk/government/organisations/"
      }
    },
    "creator": {
      "@type": "@id",
      "@id": "dcterms:creator",
      "@context": {
        "@base": "https://www.gov.uk/government/organisations/"
      }
    },
    "contact_point": {
      "@type": "vcard:Individual",
      "@id": "dcat:contactPoint",
      "@context": {
        "email": {
          "@type": "@id",
          "@id": "vcard:hasEmail",
          "@context": {
            "@base": "mailto:"
          }
        },
        "name": {
          "@id": "vcard:fn"
        },
        "telephone": {
          "@type": "@id",
          "@id": "vcard:hasTelephone",
          "@context": {
            "@base": "tel:"
          }
        }
      }
    },
    "theme": {
      "@type": "@id",
      "@id": "dcat:theme",
      "@context": {
        "@base": "https://statistics.gov.uk/id/theme/"
      }
    },
    "frequency": {
      "@type": "@id",
      "@id": "dcterms:accuralPeriodicity",
      "@context": {
        "@base": "http://purl.org/cld/freq/"
      }
    },
    "keywords": {
      "@id": "dcat:keyword"
    },
    "licence": {
      "@type": "@id",
      "@id": "dcterms:license"
    },
    "temporal_coverage": {
      "@id": "dcterms:temporal",
      "@context": {
        "start": {
          "@type": "xsd:dateTime",
          "@id": "dcat:startDate"
        },
        "end": {
          "@id": "dcat:endDate",
          "@type": "xsd:dateTime"
        }
      }
    },
    "temporal_resolution": {
      "@type": "xsd:duration",
      "@id": "dcat:temporalResolution"
    },
    "spatial_coverage": {
      "@type": "@id",
      "@id": "dcterms:spatial",
      "@context": {
        "@base": "http://statistics.data.gov.uk/id/statistical-geography/"
      }
    },
    "spatial_resolution": {
      "@type": "@id",
      "@id": "ons:spatialResolution",
      "@context": {
        "@base": "http://statistics.data.gov.uk/id/statistical-entity/"
      }
    },
    "editions": {
      "@reverse": "dcat:inSeries"
    }
  }
}
